{
  "name": "room-rental",
  "version": "1.0.0",
  "description": "\"# room-rental\"",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass-chokidar ./src -o ./src",
    "watch-css": "npm run build-css && node-sass-chokidar ./src -o ./src --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all -p build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "server": "nodemon ./server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frandm182/room-rental.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/frandm182/room-rental/issues"
  },
  "homepage": "https://github.com/frandm182/room-rental#readme",
  "dependencies": {
    "bootstrap": "^4.1.2",
    "express": "^4.16.4",
    "jquery": "^3.3.1",
    "mongoose": "^5.4.6",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "popper.js": "^1.14.3",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^2.1.3",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
